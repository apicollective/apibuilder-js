{
  "name": "apibuilder-js",
  "version": "0.0.13",
  "description": "A reference implementation of API Builder for JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "tsc --project ./tsconfig.build.json",
    "clean": "rm -rf dist",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepublishOnly": "run-s clean build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apicollective/apibuilder-js.git"
  },
  "keywords": [
    "node",
    "apibuilder",
    "referene"
  ],
  "author": "Flow Commerce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apicollective/apibuilder-js/issues"
  },
  "homepage": "https://github.com/apicollective/apibuilder-js#readme",
  "dependencies": {
    "invariant": "^2.2.4",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.9.1",
    "@types/pluralize": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base-typescript": "^1.0.0",
    "eslint-plugin-jest": "^24.4.0",
    "faker": "^5.5.3",
    "jest": "^27.1.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  }
}
